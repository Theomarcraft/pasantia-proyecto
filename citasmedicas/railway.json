{
  "build": {
    "builder": "railpack",
    "extensions": [
      "ctype",
      "curl",
      "dom",
      "fileinfo",
      "filter",
      "hash",
      "mbstring",
      "openssl",
      "pcre",
      "pdo",
      "pdo_mysql",
      "session",
      "tokenizer",
      "xml",
      "gd"
    ]
  },
  "deploy": {
    "startCommand": "php artisan serve --host=0.0.0.0 --port=$PORT"
  }
}
